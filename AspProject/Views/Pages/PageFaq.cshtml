                                </div>
                                <div id="collapse-2-@count" class="collapse" aria-labelledby="heading-2-@count"
                                     data-parent="#accordion-2">
                                    <div class="card-body">
                                        <p>@item.Content</p>
                                    </div>
                                </div>
                            </div>

                            <p class="d-none">@(count++)</p>
                        }



